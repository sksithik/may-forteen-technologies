    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "jquery": "^3.5.1",
    "bootstrap": "^4.5.0",
    "angular-bootstrap-md": "^9.3.1",
    "contentful": "^7.14.4",
