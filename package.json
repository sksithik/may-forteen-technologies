{
  "name": "may-forteen-technologies",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && ng run ng-universal-demo:server:production",
